{
  "manifest_version": 3,
  "name": "Roblox on Now.gg",
  "version": "1.0",
  "description": "Play Roblox games using Now.gg in a popup.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.ico",
      "48": "images/icon48.ico",
      "128": "images/icon128.ico"
    }
  },
  "permissions": [],
  "icons": {
    "16": "images/icon16.ico",
    "48": "images/icon48.ico",
    "128": "images/icon128.ico"
  }
}
